    <h1>Ironhack Cinema</h1>
    <p>Click on the movie to check the showtimes!</p>

<div class="moviesContainer">

    
    {{#each this.caratula}}
        <div class="peliculaCard">
            <img src="{{this.image}}" alt="caratula" height="200px">
            <h4>{{this.title}}</h4>
            <a href="/movies/{{this._id}}">See more</a>
        </div>   
    {{/each}}
    
</div>